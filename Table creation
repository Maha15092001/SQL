CREATE TABLE item( s_no INT, item_name VARCHAR(20),vegetarian VARCHAR(10),non_vegetarian VARCHAR(10));
INSERT INTO item VALUES (1,'pulao','yes','no'),(2,'briyani','no','yes'),(3,'dosa','yes','no'),(4,'sabji','yes','no');
SELECT * FROM item;
UPDATE item SET item_name = 'pani puri' WHERE s_no = 1;
SELECT * FROM item;
DELETE FROM item WHERE item_name='briyani';
SELECT * FROM item;
SELECT * FROM item WHERE vegetarian LIKE '%s';
INSERT INTO item VALUES(2,'pav baji','yes','no'),(5,'lobster fry','no','yes'),(6,'lollipop chicken','no','yes');
SELECT * FROM item;
SELECT * FROM item WHERE non_vegetarian LIKE '_e%';
DELETE FROM item WHERE item_name='lobster fry';
SELECT * FROM item;
INSERT INTO item VALUES(5,'pav baji','yes','no'),(7,'lobster fry','no','yes');
SELECT * FROM item;
SELECT DISTINCT (item_name) FROM item;
SELECT * FROM item WHERE item_name  LIKE '_a%';
DROP TABLE item;
SELECT * FROM item;
